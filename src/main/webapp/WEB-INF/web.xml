<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>EmployeeRegistrationController</servlet-name>
  	<servlet-class>com.revature.craleigh318.ers.app.employee_registration.EmployeeRegistrationController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UserLogIn</servlet-name>
  	<servlet-class>UserLogIn</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>EmployeeRegistrationController</servlet-name>
  	<url-pattern>/EmployeeRegistration</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UserLogIn</servlet-name>
  	<url-pattern>/</url-pattern>
  </servlet-mapping>
</web-app>
